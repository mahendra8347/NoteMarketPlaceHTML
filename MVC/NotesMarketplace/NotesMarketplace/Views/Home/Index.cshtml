                        <a class="navbar-brand navbar-brand-2" href="/home/index"><img src="~/Content/Front/images/homepage/logo.png" alt=""></a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="navbar-nav home-nav ml-auto">
                                <li class="nav-item">
                                    <a class="nav-link" href="/home/searchnotes">
                                        <span class="sr-only"></span>Search
                                        Notes
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/user/index">Sell Your Notes</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/home/faq">FAQ</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/home/contact">Contact Us</a>
                                </li>
                            </ul>

                            <div class="home-nav login-note-btn">
                                <a href="/account/login"><button>Login</button></a>
                            </div>
                        </div>
                    </div>
                </nav>
